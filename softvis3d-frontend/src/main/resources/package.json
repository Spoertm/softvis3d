{
  "name": "react-test",
  "version": "1.0.0",
  "description": "None given",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run install:config -s && npm run install:dist -s && npm run install:typings -s",
    "install:config": "shx cp -n config/dev.example.ts config/dev.ts",
    "install:dist": "shx cp node_modules/react/dist/react.min.js node_modules/react-dom/dist/react-dom.min.js app/static/",
    "install:typings": "typings install",
    "start": "webpack-dev-server",
    "prebuild": "npm run lint -s && npm run test -s",
    "build": "npm run build:force -s",
    "build:force": "webpack --progress --colors --prod",
    "lint": "tslint \"src/**/*.ts?(x)\" && shx echo \"[tslint] Lint successful\"",
    "test": "mocha",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "posttest:coverage": "shx rm -r .nyc_output"
  },
  "author": "Yvo Niedrich <yvo.niedrich@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "http",
    "url": "git+http://git.ungolianth.de:ungolianth/react-test.git"
  },
  "dependencies": {
    "axios": "^0.15.1",
    "i18next": "^3.4.3",
    "mobx": "^2.5.2",
    "mobx-react": "^3.5.7",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-i18next": "^1.10.1",
    "sass-loader": "^4.0.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2",
    "typings": "^1.4.0",
    "webpack": "^1.13.2",
    "jquery": "^3.1.1",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.1.0",
    "nyc": "^8.3.1",
    "react-addons-test-utils": "^15.3.2",
    "shx": "^0.1.4",
    "sinon": "^1.17.6",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.9.0",
    "webpack-dev-server": "^1.16.1"
  }
}
